{
     
      "key" : "infoSimple",
      "wrappers" : ["form-field-cfg-all"],
      "templateOptions": {
        "addText": "Add another item"
      },
	  
      "fieldGroup": [
          {
            "className": "col-sm-6",
            "type": "input",
            "key": "key",
            "templateOptions": {
              "label": "f-key",
              "description": "valore key identificatore anche per essere utilizzato come riferimento nelle espressioni (sola lettura)",
              "disabled" : true,
              "required": true
            }
          },
          {
            "type": "input",
            "key": "type",
            "className": "col-sm-6",
            "templateOptions": {
              "description": "tipo della chiave (sola lettura)",
              "label": "f-type",
              "disabled" : true,
              "required": true
            }
          },
          

          {
           
            "key" : "templateOptions",
            "fieldGroup": [
              {
                "className": "col-md-4",
                "type": "input",
                "key": "label",
                "templateOptions": {
                  "label": "label H1",
                  "required": true
                }
              },
              {
                "type": "input",
                "key": "description",
                "className": "col-md-4",
                "templateOptions": {
                  "label": "description H2"
                }
              }
             ]
          },

  

    {
            "type": "checkbox",
            "key": "toRemove",
            "className": "col-sm-4",
            "templateOptions": {
              "label": "selezionare per eliminare questo campo"
            }
    }

    ]

}